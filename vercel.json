{
    "rewrites": [
      { "source": "/check-out/:id", "destination": "/index.html" },
      { "source": "/details/:id", "destination": "/index.html" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
        ]
      }
    ],
    "cleanUrls": true
}

  